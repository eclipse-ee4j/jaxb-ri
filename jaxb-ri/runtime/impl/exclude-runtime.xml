<FindBugsFilter>

    <!--
    This is as designed for performance reasons.
    -->
    <Match>
        <Or>
            <Class name="com.sun.xml.bind.util.AttributesImpl"/>
            <Class name="com.sun.xml.bind.v2.runtime.NameBuilder"/>
            <Class name="com.sun.xml.bind.v2.runtime.unmarshaller.TagName"/>
            <Class name="com.sun.xml.bind.v2.schemagen.Util"/>
            <Class name="com.sun.xml.bind.v2.util.QNameMap"/>
        </Or>
        <Bug pattern="ES_COMPARING_PARAMETER_STRING_WITH_EQ"/>
    </Match>
    
    <Match>
        <Class name="com.sun.xml.bind.v2.runtime.unmarshaller.UnmarshallingContext$State"/>
        <Bug pattern="NP_NULL_ON_SOME_PATH"/>
    </Match>

    <Match>
        <Bug pattern="DM_DEFAULT_ENCODING"/>
    </Match>
</FindBugsFilter>
